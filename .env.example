# Azure Document Intelligence credentials
# Copy this file to .env and fill in your values
AZURE_DI_ENDPOINT=https://YOUR_RESOURCE_NAME.cognitiveservices.azure.com
AZURE_DI_KEY=YOUR_API_KEY

# Google Document AI credentials
# Processor name format: projects/{project}/locations/{region}/processors/{processor_id}
# Region examples: us, eu, asia
GOOGLE_DOCAI_PROCESSOR_NAME=projects/YOUR_PROJECT_ID/locations/YOUR_REGION/processors/YOUR_PROCESSOR_ID
GOOGLE_DOCAI_CREDENTIALS_PATH=/path/to/YOUR_SERVICE_ACCOUNT.json
